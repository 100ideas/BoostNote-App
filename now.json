{
  "name": "boostnote.next",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/app/storages/.*",
      "dest": "/index.html"
    },
    {
      "src": "/app/?(.*)",
      "dest": "/$1"
    }
  ]
}
