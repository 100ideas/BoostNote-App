{
  "name": "inpad",
  "productName": "Inpad",
  "version": "0.1.0",
  "description": "A Polished Notes App with Github Flavored Markdown",
  "main": "app/index.js",
  "scripts": {
    "start": "electron app/index.js",
    "start-dev": "NODE_ENV=development electron app/index.js",
    "compile": "NODE_ENV=production webpack --config webpack.config.js",
    "pack": "env-cmd ./.env build --dir",
    "dist": "env-cmd ./.env build",
    "lint": "standard",
    "webpack": "NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "test:run": "NODE_ENV=test electron ./tools/webpack-test.js",
    "test:serve": "NODE_ENV=test webpack-dev-server --config webpack.config.js",
    "rebuild": "electron-rebuild"
  },
  "keywords": [
    "markdown",
    "snippet",
    "notes",
    "gfm"
  ],
  "author": "Sarah Seo <sarah.seo.0311@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.7",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "electron": "^1.4.12",
    "electron-builder": "^10.12.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-rebuild": "^1.3.0",
    "env-cmd": "^4.0.0",
    "json-loader": "^0.5.4",
    "pouchdb-adapter-memory": "^6.1.0",
    "react-desktop": "^0.2.14",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "standard": "^8.5.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^2.1.0-beta.11"
  },
  "dependencies": {
    "codemirror": "^5.20.2",
    "color": "^0.11.4",
    "electron-auto-updater": "^0.8.3",
    "electron-positioner": "^3.0.0",
    "filenamify": "^1.2.1",
    "github-markdown-css": "^2.4.1",
    "immutable": "^3.8.1",
    "katex": "^0.6.0",
    "lodash": "^4.16.6",
    "moment": "^2.17.0",
    "octicons": "^5.0.1",
    "pouchdb": "^6.0.7",
    "pouchdb-browser": "^6.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "remark": "^6.2.0",
    "remark-emoji": "^1.1.1",
    "remark-html": "^5.1.0",
    "remark-lint": "^5.2.0",
    "remark-slug": "^4.2.2",
    "sander": "^0.5.1",
    "strip-markdown": "^3.0.0",
    "styled-components": "^1.0.10",
    "unist-util-visit": "^1.1.1"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "build": {
    "appId": "com.electron.Inpad",
    "copyright": "Copyright Â© 2016-2017 Sarah Seo",
    "category": "public.app-category.productivity",
    "files": [
      "app",
      "compiled",
      "resources",
      "node_modules"
    ],
    "asar": false,
    "mac": {
      "category": "public.app-category.productivity",
      "target": "default",
      "icon": "resources/icon.icns"
    },
    "dmg": {
      "background": "resources/dmg.png",
      "contents": [
        {
          "x": 192,
          "y": 344
        },
        {
          "x": 448,
          "y": 344,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "window": {
        "x": 0,
        "y": 0,
        "width": 640,
        "height": 480
      }
    }
  }
}
